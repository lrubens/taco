{
    "application_name": "cpp_taco_TTV",
    "design_of_experiment": {
        "doe_type": "random sampling",
        "number_of_samples": 8
    },
    "baco_mode": {
        "mode": "client-server"
    },
    "input_parameters": {
        "chunk_size_fpos": {
            "parameter_type": "ordinal",
            "parameter_default": 2,
            "transform": "log",
            "values": [2, 4, 8, 16, 32, 64, 128, 256, 512, 1024]
        },
        "chunk_size_i": {
            "parameter_type": "ordinal",
            "parameter_default": 4,
            "transform": "log",
            "values": [2, 4, 8, 16, 32, 64, 128, 256, 512, 1024]
        },
        "chunk_size_k": {
	    "constraints": [
                "chunk_size_k >= 2"
            ],
            "parameter_type": "ordinal",
            "parameter_default": 8,
            "transform": "log",
            "values": [1, 2, 4, 8, 16, 32, 64, 128, 256, 512, 1024]
        },
        "omp_chunk_size": {
            "parameter_type": "ordinal",
            "parameter_default": 1,
            "transform": "log",
            "values": [
                1,
                2,
                4,
                8,
                16,
                32,
                64,
                128,
                256
            ]
        },
        "omp_num_threads": {
            "parameter_type": "ordinal",
            "parameter_default": 32,
            "transform": "log",
            "values": [
                1,
                2,
                4,
                8,
                16,
                32,
                64
            ]
        },
        "omp_scheduling_type": {
            "parameter_default": 0,
            "parameter_type": "ordinal",
            "values": [0, 1]
        },
        "permutation": {
            "parameter_type": "permutation",
            "parametrization": "spearman",
            "parameter_default": [0,1,2,3,4],
            "values": [
                5
            ]
        }
    },
    "models": {
        "model": "gaussian_process"
    },
    "lengthscale_prior" : {
	  "name": "gamma",
	  "parameters": [1.3, 0.1]
    },
    "log_transform_output": true,
    "epsilon_greedy_threshold": 0,
    "run_directory": "/home/taco/build",
    "GP_model": "gpy",
    "multistart_hyperparameter_optimization": true,
    "hyperparameter_optimization_iterations": 10
}
