{
    "application_name": "cpp_taco_SpMM",
    "design_of_experiment": {
        "doe_type": "random sampling",
        "number_of_samples": 11
    },
    "epsilon_greedy_threshold": 0,
    "hypermapper_mode": {
        "mode": "client-server"
    },
    "input_parameters": {
        "chunk_size": {
            "parameter_default": 512,
            "parameter_type": "ordinal",
            "values": [
                2,
                4,
                8,
                16,
                32,
                64,
                128,
                256,
                512
            ]
        },
        "omp_chunk_size": {
            "parameter_default": 256,
            "parameter_type": "ordinal",
            "values": [
                1,
                2,
                4,
                8,
                16,
                32,
                64,
                128,
                256
            ]
        },
        "omp_num_threads": {
            "parameter_default": 32,
            "parameter_type": "ordinal",
            "values": [
                1,
                2,
                4,
                8,
                16,
                32
            ]
        },
        "omp_scheduling_type": {
            "parameter_default": 0,
            "parameter_type": "ordinal",
            "values": [
                0,
                1
            ]
        },
        "permutation": {
            "constraints": [
                "permutation_i0 < permutation_i3",
                "permutation_i1 < permutation_i3",
                "permutation_i0 < permutation_i2",
                "permutation_i1 < permutation_i2",
                "(permutation_i3 < permutation_i2) | (permutation_i3 < permutation_i4)"
            ],
            "parameter_default": [
                0,
                1,
                2,
                3,
                4
            ],
            "parameter_type": "permutation",
            "parametrization": "spearman",
            "values": [
                5
            ]
        },
        "unroll_factor": {
            "constraints": [
                "unroll_factor < chunk_size"
            ],
            "dependencies": [
                "chunk_size"
            ],
            "parameter_default": 256,
            "parameter_type": "ordinal",
            "values": [
                2,
                4,
                8,
                16,
                32,
                64,
                128,
                256,
                512
            ]
        }
    },
    "lengthscale_prior" : {
	"name": "gamma",
	"parameters": [1.3, 0.1]
    },
    "log_file": "experiments/outdata_SpMM_bcsstk18/bayesian_optimization/log_cpp_taco_SpMM.log",
    "log_transform_output": false,
    "models": {
        "model": "gaussian_process"
    },
    "allow_duplicate_samples": true,
    "optimization_iterations": 110,
    "optimization_method": "bayesian_optimization",
    "optimization_objectives": [
        "compute_time"
    ],
    "run_directory": "~TACO/taco/build",
    "GP_model": "gpy",
    "multistart_hyperparameter_optimization": true,
    "hyperparameter_optimization_iterations": 10
}
